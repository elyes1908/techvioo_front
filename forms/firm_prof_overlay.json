                            "value": "<style>\r\n\r\n#html #section-actions,.form-section-title,#field26{\r\n    display: none;\r\n}\r\n\r\n#websitediv{\r\n    cursor:pointer;\r\n}\r\n\r\n<\/style>\r\n<script>\r\n    function SetProfileImage() {\r\n        let image = document.getElementById(\"profile_image\").value;\r\n        let pro_image = document.getElementById(\"pro_img\");\r\n        pro_image.src = \"\"+image;\r\n        pro_image.addEventListener(\"error\", imageError = () => {\r\n            pro_image.onerror = \"\";\r\n            pro_image.src = \"#appResource.StartupDefault.jpg#\";\r\n            return true;\r\n        });\r\n    }\r\n    \r\n    $( document ).ready(function() {\r\n        \r\n    document.getElementById('nameP').innerHTML=document.getElementById('name').value;\r\n    document.getElementById('headP').innerHTML=document.getElementById('headquarters').value;\r\n    document.getElementById('locationP').innerHTML=document.getElementById('location').value;\r\n    document.getElementById('employeesP').innerHTML=document.getElementById('num_employees').value;\r\n    document.getElementById('industryP').innerHTML=document.getElementById('industry').value;\r\n    document.getElementById('typeP').innerHTML=document.getElementById('type').value;\r\n    document.getElementById('foundedP').innerHTML=document.getElementById('founded_date').value;\r\n    document.getElementById('descriptionP').innerHTML=document.getElementById('company_description').value;\r\n    document.getElementById('linkedindiv').innerHTML=document.getElementById('url').value.replace('https://www.linkedin.com','');\r\n    document.getElementById('websitediv').innerHTML=document.getElementById('website').value.replace('http://','');\r\n    document.getElementById('phonediv').innerHTML=document.getElementById('phone').value;\r\n   insertSkills();\r\n   redirectWebsite();\r\n   SetProfileImage();\r\n\r\n});\r\n    $('#fullname,#job_company,#location,#score,#email,#facebook,#phone,#linkedin,#twitter').parent('div').addClass('d-none');\r\n    \r\n    $(\"#section2\").addClass('profile-pop-div col-md-3 bg-white  py-4 border  px-4').removeClass('form-section');\r\n           \r\n      $(\"#section11\").addClass('profile-pop-div  profile-pop-details');\r\n\r\n     \r\n      $(\"#firm_prof_overlay #skills\").addClass('profile-pop-div  profile-pop-details');\r\n      $('#firm_prof_overlay .profile-pop-div').wrapAll('  <div class=\" d-inline-flex flex-column flex-md-row w-100 w-100 min-height-80vh\"><\/div>');\r\n      $('#prof_contact').wrapAll('  <div class=\"col-md-4   px-2\"><\/div>');\r\n      $('#firm_prof_overlay .profile-pop-details').wrapAll('  <div class=\"col-md-9   px-0 mt-2 px-md-2 mt-md-0\"><div class=\"bg-white border h-100  py-4 px-4\"><\/div><\/div>');\r\n    \r\n        $('#headerOverlay').wrapAll('  <div class=\"col-md-12   px-2 bg-white  mb-3\"><\/div>');\r\n        \r\n\r\n        \r\n     \r\n\r\n\r\n\r\n\r\n\r\n\r\n  function insertSkills(){\r\n      var pp3='';\r\n    pp3 = document.getElementById('specialities').value;\r\n    if(pp3 !='' && pp3!=null){\r\n    pp2=pp3.replace('[','');\r\n    pp=pp2.replace(']','');\r\n    pp=pp.split(',');\r\n    skills=pp;\r\n    let counter=0;\r\n    let list = document.getElementById('skill_list');\r\n    // console.log(list)\r\n    for (const skill in skills) {\r\n      const li = document.createElement('li');\r\n      li.innerHTML = `${skills[skill]}`;\r\n      list.appendChild(li);\r\n    }\r\n        $(\"ul#skill_list li\").addClass('float-start mx-1 mb-2 px-3 py-2 btn-yellow rounded-2 fs-5 epilogue-semiBold');\r\n      $(\"ul#skill_list li\").css('float','left')\r\n        \r\n    }else{\r\n        document.getElementById('specialtiesDiv').parentNode.style.display=\"none\";\r\n    }\r\n   \r\n  }\r\n\r\n\r\n\r\n      function redirectWebsite() {\r\n        var web = document.getElementById('websitediv').parentNode;\r\n        web.addEventListener(\"click\", function () {\r\n            window.open(document.getElementById('website').value, '_blank');\r\n        })\r\n    }\r\n    \r\n    function openLinkedinProfile(){\r\n        var LiDiv = document.getElementById('linkedindiv').parentNode;\r\n        LiDiv.addEventListener(\"click\", function () {\r\n            window.open(document.getElementById('url').value, \"_blank\");\r\n        })\r\n      \r\n    }\r\n    \r\n    function buttonLinkedinProfile(){\r\n         window.open(document.getElementById('url').value, \"_blank\");\r\n    }\r\n\r\n<\/script>\r\n<\/head>\r\n\r\n"
