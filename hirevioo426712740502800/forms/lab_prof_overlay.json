                "script": "\nimport org.apache.commons.lang3.ArrayUtils;\nimport org.joget.apps.form.model.*;\nimport org.joget.apps.form.service.*;\nimport org.joget.apps.app.service.*;\nimport java.sql.*;\nimport java.util.*;\nimport java.text.*;\nimport java.sql.Connection;\nimport java.sql.PreparedStatement;\nimport java.sql.ResultSet;\nimport java.sql.SQLException;\nimport javax.sql.DataSource;\nimport java.sql.Date;  \nimport org.apache.http.HttpResponse;\nimport org.apache.http.client.methods.HttpGet;\nimport org.apache.http.client.methods.HttpRequestBase;\nimport org.apache.http.impl.client.CloseableHttpClient;\nimport org.apache.http.impl.client.HttpClients;\nimport org.apache.http.HttpEntity;\nimport org.apache.http.util.EntityUtils;\nimport java.io.IOException;\nimport java.nio.charset.StandardCharsets;\nimport java.net.URLEncoder;\n\n\npublic String getIndexes(){\n    String id_search=\"#requestParam.search#\";\n    String res=\"\";\n    //Get Joget's current datasource configs\n    DataSource ds = (DataSource) AppUtil.getApplicationContext().getBean(\"setupDataSource\");\n     \n    con = ds.getConnection();\n     \n    if (!con.isClosed()) {\n        //Get the URL parameter\n        String connectedUser=\"#currentUser.username#\";\n      \n        //Here you can query from one or multiple tables using JOIN etc\n        String sql = \"SELECT id_profile FROM app_fd_recherche_inter WHERE id_recherche='\"+id_search+\"'\";\n        PreparedStatement stmt = con.prepareStatement(sql);\n\n        //Execute the SELECT SQL statement\n        ResultSet rs = stmt.executeQuery();\n             \n        //Get value from columns of record(s)\n        while (rs.next()) {\n           if(res==\"\"){\n               res=rs.getString(1);\n           }else{\n               res+=\"-\"+rs.getString(1);\n           }\n          \n        }\n        con.close();\n        return res;\n    }\n    \n}\n\n\n\nString id=\"#requestParam.id#\";\nString id_search=\"#requestParam.search#\";\nString overlay=getIndexes();\nString previous=\"\";\nString next=\"\";\nif(overlay.contains(\"-\")){\n    String [] indices=overlay.split(\"-\");\n    int lengthArray=(indices.length)-1;\n    int index = ArrayUtils.indexOf(indices, id);\n    System.out.println(\"******** index: \"+index);\n    if(index == 0){\n        previous=indices[index];\n        next=indices[index+1];\n    }else if(index == lengthArray){\n        previous=indices[index-1];\n        next=indices[index];\n    }else{\n        previous=indices[index-1];\n        next=indices[index+1];\n    }\n}else{\n    previous=overlay;\n    next=overlay;\n}\n\nSystem.out.println(\"******************PREVIOUS:\"+previous);\nSystem.out.println(\"******************NEXT: \"+next);\n\nFormRowSet f = new FormRowSet();\nf.setMultiRow(true);\n \n//Get Joget's current datasource configs\nDataSource ds = (DataSource) AppUtil.getApplicationContext().getBean(\"setupDataSource\");\n \ncon = ds.getConnection();\n \nif (!con.isClosed()) {\n    //Get the URL parameter\n    String recordId = \"#requestParam.id#\";\n    \n    //Here you can query from one or multiple tables using JOIN etc\n    String sql = \"SELECT * FROM app_fd_lab_profile WHERE id=?\";\n    PreparedStatement stmt = con.prepareStatement(sql);\n    stmt.setString(1, recordId);\n  \n    //Execute the SELECT SQL statement\n    ResultSet rs = stmt.executeQuery();\n         \n    //Get value from columns of record(s)\n    while (rs.next()) {\n        FormRow r1 = new FormRow();\n        \n        if(rs.getString(8)!=null)\n            r1.put(\"profile_image\",\"data:image/png;base64, \"+rs.getString(8));\n        if(rs.getString(19)!=null)\n            r1.put(\"specialities\", rs.getString(19));\n        if(rs.getString(15)!=null)\n            r1.put(\"website\", rs.getString(15));\n        if(rs.getString(20)!=null)\n            r1.put(\"url\",rs.getString(20));\n        if(rs.getString(9)!=null)\n            r1.put(\"name\", rs.getString(9));\n        if(rs.getString(21)!=null)\n            r1.put(\"founded_date\",rs.getString(21));\n        if(rs.getString(10)!=null)\n            r1.put(\"company_description\",rs.getString(10));\n        if(rs.getString(16)!=null)\n            r1.put(\"type\",rs.getString(16));\n        if(rs.getString(18)!=null)\n            r1.put(\"institution\",rs.getString(18));\n        if(rs.getString(11)!=null)\n            r1.put(\"discipline\",rs.getString(11));\n        if(rs.getString(13)!=null)\n            r1.put(\"phone\",rs.getString(13));\n        if(rs.getString(12)!=null)\n            r1.put(\"location\",rs.getString(12));\n \n        if(rs.getString(17)!=null)\n            r1.put(\"num_employees\",rs.getString(17));\n        r1.put(\"previous\", previous);\n        r1.put(\"next\", next);\n        f.add(r1);\n    }\n}\ncon.close();\nreturn f;\n\n"
