                            "value": "<style>\n#jqueryDialogDiv body.horizontal_menu #main {\n    margin-top: 0 !important;\n}\n#section-actions{\n    display:none;\n}\n\n  a{\n      text-decoration: none;\n  }\n    .previous, .next {\n   background-color: #0E6BB2;\n    color: white;\n    height: 30px;\n    display: inline-block;\n    width: 28px;\n    font-size: 19px;\n    padding: 0;\n    line-height: 30px;\n    text-align: center;\n    margin-bottom:5px;\n    }\n    \n.previous, .next {\n    font-size: 21px !important;\n}\n    \n    .round {\n      border-radius: 50%;\n    }\n    \n#nextP{\n    width:99.6%;\n}\n   \n<\/style>\n<div id=\"nextP\" class=\"d-flex justify-content-between p-3 align-items-center border bg-white mb-2\">\n<a href=\"#\" id=\"btnPrev\" class=\"previous round\" onclick=previous() >&#8249;<\/a>\n<h3 id=\"search_title\" class=\"template-color\"><\/h3>\n<a href=\"#\" id=\"btnNext\" class=\"next round\" onclick=next()>&#8250;<\/a>\n<\/div>\n<script>\n\n    let nextClicked=false;\n    let previousClicked=false;\n    function next(){\n        nextClicked=true;\n        const queryString = window.location.search;\n        const urlParams = new URLSearchParams(queryString);\n        const title = urlParams.get('title');\n        const id_search = urlParams.get('search');\n        var nextIndex=document.getElementById('next').value;\n        var uri = \"lab_preview?id=\"+nextIndex+\"&title=\"+title+\"&search=\"+id_search+\"&embed=true\";\n        window.location.replace(uri);\n    }\n    \n    function previous(){\n        previousClicked=true;\n        const queryString = window.location.search;\n        const urlParams = new URLSearchParams(queryString);\n        const title = urlParams.get('title');\n        const id_search = urlParams.get('search');\n        var previousIndex=document.getElementById('previous').value;\n        var uri = \"lab_preview?id=\"+previousIndex+\"&title=\"+title+\"&search=\"+id_search+\"&embed=true\";\n        window.location.replace(uri);\n    }\n    \n    \n    $( document ).ready(function() {\n        const queryString = window.location.search;\n        const urlParams = new URLSearchParams(queryString);\n        const id = urlParams.get('id');\n        const title = urlParams.get('title');\n        document.getElementById('search_title').innerHTML=title;\n        var nextIndex=document.getElementById('next').value;\n        var previousIndex=document.getElementById('previous').value;\n        console.log(\"KIFKIF: \"+nextIndex);\n        console.log(\"KIFKIF \"+previousIndex);\n        if(id==nextIndex){\n            document.getElementById(\"btnNext\").style.pointerEvents = \"none\";\n        }else if (id==previousIndex) {\n            document.getElementById(\"btnPrev\").style.pointerEvents = \"none\";\n        }\n        else{\n            document.getElementById(\"btnNext\").style.pointerEvents = \"auto\";\n            document.getElementById(\"btnPrev\").style.pointerEvents = \"auto\";\n        }\n        \n        if (previousIndex==nextIndex) {\n            console.log(\"KIFKIF\");\n            document.getElementById(\"btnPrev\").style.pointerEvents = \"none\";\n            document.getElementById(\"btnNext\").style.pointerEvents = \"none\";\n        }\n        \n    \n\n    });\n\n<\/script>"
