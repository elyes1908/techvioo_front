#515B6F !important;\r\n        transition: all 0.3s ease;\r\n   }\r\n   .menu-link:hover{\r\n       color:#A85FFF !important;\r\n   }\r\n    .menu-block {\r\n        display: inline-block;\r\n        font-family: inherit;\r\n        font-weight: 500;\r\n        line-height: 1.25;\r\n        user-select: none;\r\n        text-align: center;\r\n        white-space: nowrap;\r\n        padding: 0.65rem 1.35rem;\r\n        transition: all 0.3s ease-in-out;\r\n   }\r\n   .started-btn,.signup-btn {\r\n       background-color:#A85FFF;\r\n       border:1px solid #A85FFF;\r\n       color:white;\r\n   }\r\n   \r\n   .login-btn:hover{\r\n       border:none;\r\n   }\r\n   .login-btn,.signup-btn,.started-btn:hover{\r\n       border:1px solid #0E6BB2;\r\n       background-color:#fff;\r\n       color:#0E6BB2;\r\n   }\r\n   a{\r\n       text-decoration:none!important;\r\n   }\r\n   a.login-btn {\r\n        background-color:#0E6BB2 !important;\r\n        color:white!important;\r\n        border-radius: 37px;\r\n        font-size: 16px !important;\r\n        border: none;\r\n   }\r\n   \r\n   a.menu-block.login-btn.rounded-4.text-decoraiotn-none {\r\n    background: linear-gradient(180deg, #A85FFF 0%, #EC75E7 99.98%, rgba(146, 54, 255, 0.569132) 99.99%, rgba(116, 0, 255, 0) 100%) !important;\r\n    padding: 16px 20px 16px 15px;\r\n    }\r\n   #sign_in a.tag.menu-block.login-btn.rounded-4.text-decoraiotn-none {\r\n    font-weight: 400 !important;\r\n    color: #666666 !important;\r\n    padding: 0 !important;\r\n  \r\n}\r\n    @media only screen and (min-width: 48rem) {\r\n        .menu {\r\n            position: relative;\r\n            top: 0;\r\n            left: 0;\r\n            width: auto;\r\n            height: auto;\r\n            background: none;\r\n            box-shadow: none;\r\n            width:44%;\r\n       }\r\n\r\n       a.tag.menu-block.login-btn.rounded-4.text-decoraiotn-none {\r\n           background: #f8f9fd !important;\r\n            color: #15074F !important;\r\n            padding: 16px 40px !important;\r\n        }\r\n        .menu-inner {\r\n            display: flex;\r\n            flex-direction: row;\r\n            column-gap: 50px;\r\n            margin: 0 auto;\r\n            width: 60%;\r\n            justify-content: center !important;\r\n       }\r\n        .menu-link {\r\n            text-transform: capitalize;\r\n       }\r\n        .menu-block {\r\n            margin-left: 2rem;\r\n       }\r\n   }\r\n    .burger {\r\n        position: relative;\r\n        display: block;\r\n        cursor: pointer;\r\n        order: -1;\r\n        width: 1.75rem;\r\n        height: auto;\r\n        border: none;\r\n        outline: none;\r\n        visibility: visible;\r\n   }\r\n    .burger-line {\r\n        display: block;\r\n        cursor: pointer;\r\n        width: 100%;\r\n        height: 2px;\r\n        margin: 6px auto;\r\n        transform: rotate(0deg);\r\n        background-color: black;\r\n        transition: all 0.3s ease-in-out;\r\n   }\r\n    @media only screen and (min-width: 48rem) {\r\n        .burger {\r\n            display: none;\r\n            visibility: hidden;\r\n       }\r\n   }\r\n    .overlay {\r\n        position: fixed;\r\n        top: 0;\r\n        left: 0;\r\n        width: 100%;\r\n        height: 100%;\r\n        z-index: 9;\r\n        opacity: 0;\r\n        visibility: hidden;\r\n        background-color: rgba(0, 0, 0, 0.65);\r\n        transition: all 0.3s ease-in-out;\r\n   }\r\n    .overlay.is-active {\r\n        display: block;\r\n        opacity: 1;\r\n        visibility: visible;\r\n   }\r\n    header.navbar{\r\n        display:none;\r\n    }\r\n    \r\n    #landing_page .navbar #btn-menu-login {\r\n    display: flex;\r\n}\r\n\r\n#landing_page .navbar #btn-menu-login input.form-control.border-0.bg-light {\r\n    height: 50px;\r\n    background: #FFFFFF !important;\r\n    border-radius: 37px;\r\n}\r\n\r\n\r\n\r\n#landing_page nav.navbar {\r\n    background: #F8F9FD;\r\n}\r\n\r\n\r\n